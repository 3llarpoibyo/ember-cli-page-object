{
  "name": "ember-cli-page-object",
  "version": "1.1.0",
  "description": "This ember-cli addon eases the construction of page objects on your acceptance tests",
  "homepage": "http://ember-cli-page-object.js.org",
  "bugs": "https://github.com/san650/ember-cli-page-object/issues",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "preversion": "git clean -df && npm test",
    "start": "ember server",
    "test": "ember try:testall && mocha tests/**/*-test-node.js",
    "docs": "node ./docs",
    "mocha": "mocha tests/**/*-test-node.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/san650/ember-cli-page-object.git"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "Santiago Ferreira",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.2.0",
    "chai": "^3.5.0",
    "documentation": "^3.0.4",
    "ember-ajax": "0.7.1",
    "ember-cli": "2.4.2",
    "ember-cli-app-version": "^1.0.0",
    "ember-cli-dependency-checker": "^1.2.0",
    "ember-cli-doc-server": "1.1.0",
    "ember-cli-htmlbars": "^1.0.1",
    "ember-cli-htmlbars-inline-precompile": "^0.3.1",
    "ember-cli-inject-live-reload": "^1.3.1",
    "ember-cli-qunit": "^1.2.1",
    "ember-cli-release": "0.2.8",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-disable-proxy-controllers": "^1.0.1",
    "ember-export-application-global": "^1.0.4",
    "ember-load-initializers": "^0.5.0",
    "ember-resolver": "^2.0.3",
    "ember-try": "^0.1.2",
    "loader.js": "^4.0.0",
    "mocha": "^2.4.5"
  },
  "keywords": [
    "acceptance",
    "integration",
    "components",
    "ember",
    "ember-addon",
    "ember-cli",
    "helper",
    "page object",
    "page-object",
    "pageobject",
    "testing"
  ],
  "dependencies": {
    "ember-cli-babel": "^5.1.5"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
